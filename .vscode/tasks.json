{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "catkin_make: build",
      "type": "shell",
      "command": "catkin_make",
      "args": [
        "-DCMAKE_BUILD_TYPE=Debug"
      ],
      "options": {
        "cwd": "/home/fieldro/Dev/ZL_B00"
      },
      "problemMatcher": [
        "$gcc"
      ],
      "group": {
        "kind": "build",
        "isDefault": true
      }
    },
    {
      "type": "cppbuild",
      "label": "C/C++: gcc build active file",
      "command": "/usr/bin/gcc",
      "args": [
        "-fdiagnostics-color=always",
        "-g",
        "${file}",
        "-o",
        "${fileDirname}/${fileBasenameNoExtension}"
      ],
      "options": {
        "cwd": "${fileDirname}"
      },
      "problemMatcher": [
        "$gcc"
      ],
      "group": "build"
    }
  ]
}
